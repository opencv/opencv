# CMake script to replace items
# in sources generated by glib-mkenums

FILE(READ ${ENUM_INPUT_SRC} enum_in)

STRING(REPLACE
  "_t_get_type"
  "_get_type"
  enum_out_tmp
  "${enum_in}"
  )

STRING(REPLACE
  "_T ("
  " ("
  enum_out
  "${enum_out_tmp}"
  )

FILE(WRITE ${ENUM_OUTPUT_SRC} "${enum_out}")
FILE(REMOVE ${ENUM_INPUT_SRC})