layer {
  name: "bw_conv1_1"
  type: "Convolution"
  bottom: "data_l"
  top: "conv1_1"
  # param {lr_mult: 0 decay_mult: 0} 
  # param {lr_mult: 0 decay_mult: 0} 
  convolution_param {
    num_output: 64
    pad: 1
    kernel_size: 3
  }
}
